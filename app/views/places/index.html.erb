
<%@places.each do |place| %>
    <div class="frost-box col-10 offset-1">
        <h1> <%= place.name %> </h1>
        <i> <%= place.address %> </i> <br/> <br/>
        <p> <%= place.description %> </p><br/>
    </div>
<% end %>

<div class="pagination-location  col-6 offset-6>
    <%= will_paginate @places %>
</div>